<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <TextView
        android:id="@+id/tv_title_login_accepted"
        android:layout_width="145dp"
        android:layout_height="52dp"
        android:layout_marginTop="120dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="@string/tv_title_login_accepted"
        android:textColor="#009688"
        android:textSize="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_accepted"
        android:layout_width="212dp"
        android:layout_height="50dp"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="@string/tv_accepted"
        android:textIsSelectable="true"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/tv_title_login_accepted"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="@+id/tv_title_login_accepted"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_login_accepted" />

